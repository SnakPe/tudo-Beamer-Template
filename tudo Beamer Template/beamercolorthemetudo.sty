% footline
\setbeamercolor*{footlinecolorUp}{bg=TUGreen}
\setbeamercolor*{footlinecolorDown}{bg=TUWhite, fg=black}
% headline
\setbeamercolor*{headlinecolorUp}{bg=TUGreen,fg=TUWhite}
\setbeamercolor*{headlinecolorDown}{bg=TUDarkGreen,fg=TUWhite}