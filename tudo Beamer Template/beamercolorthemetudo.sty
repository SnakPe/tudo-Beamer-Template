% colors stolen from the tu-dortmund.de website
\definecolor{TUGreen}{RGB}{131, 184, 24}
\definecolor{TUDarkGreen}{HTML}{639a00}
\definecolor{TUWhite}{RGB}{244, 244, 244}
\definecolor{TUGrey}{HTML}{3b3b3b}
\definecolor{TULightOrange}{HTML}{d89427}
\definecolor{TUDarkOrange}{HTML}{ca7406}

%text
\setbeamercolor{normal text}{fg=TUGrey,bg=TUWhite}
\setbeamercolor*{author}{fg=black}
\setbeamercolor*{institute}{fg=black}
\setbeamercolor*{titlelike}{fg = TUDarkGreen}
\setbeamercolor*{frametitle}{fg = TUGreen}
\setbeamercolor*{section in toc}{fg = TUGreen}
\setbeamercolor*{subsection in toc}{fg = TUGrey}


\setbeamercolor*{background canvas}{bg = TUWhite}
\setbeamercolor*{item}{fg=TUDarkOrange}
\setbeamercolor*{subitem}{fg=TULightOrange}
